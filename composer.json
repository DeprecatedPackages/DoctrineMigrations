{
	"name": "zenify/doctrine-migrations",
	"description": "Implementation of Doctrine Migrations to Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.5",
		"doctrine/migrations": "~1.0@dev",
		"tracy/tracy": "~2.2"
	},
	"suggest": {
		"kdyby/doctrine": "Integration of Doctrine 2 ORM into Nette Framework",
		"kdyby/console": "Integration of Symfony Console component into Nette Framework"
	},
	"require-dev": {
		"kdyby/doctrine": ">=2.0@dev",
		"kdyby/console": ">=2.0@dev",
		"nette/bootstrap": "~2.2",
		"nette/di": "~2.2",
		"nette/http": "~2.2",
		"nette/mail": "~2.2",
		"nette/safe-stream": "~2.2"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineMigrations\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineMigrations\\Tests\\": "tests"
		}
	}
}
