{
	"name": "zenify/doctrine-migrations",
	"description": "Implementation of Doctrine Migrations to Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": "^5.6|^7.0",
		"doctrine/migrations": "^1.0",
		"nette/di": "^2.3",
		"symfony/console": "^2.7|^3.0",
		"symnedi/event-dispatcher": "^0.3.1|^0.4.0"
	},
	"require-dev": {
		"kdyby/console": "^2.4",
		"kdyby/events": "^3.0",
		"kdyby/doctrine": "^3.1",
		"nette/bootstrap": "^2.3",
		"nette/reflection": "^2.3",
		"phpunit/phpunit": "^5.4",
		"tracy/tracy": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineMigrations\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineMigrations\\Tests\\": "tests"
		}
	}
}
